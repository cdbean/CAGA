<Window x:Class="CAGA.Map.SymbologyWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="SymbologyWindow" Height="396" Width="540" WindowStartupLocation="CenterScreen" WindowStyle="ToolWindow">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="400" />
            <ColumnDefinition/>
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition/>
            <RowDefinition Height="80" />
        </Grid.RowDefinitions>
        <Grid Name="SymbolCtrlGrid" Grid.Column="0" Grid.RowSpan="2"></Grid>
        <StackPanel Grid.Column="1" Grid.Row="1" Margin="10">
            <Button Name="OKBtn" Content="OK" HorizontalAlignment="Stretch" Margin="5" Click="OKBtn_Click"></Button>
            <Button Name="CancelBtn" Content="Cancel" Margin="5" Click="CancelBtn_Click"></Button>
        </StackPanel>
        <Image Grid.Column="1" Grid.Row="0" Name="PreviewImage" Width="100" Height="100" VerticalAlignment="Top" />
    </Grid>
</Window>
